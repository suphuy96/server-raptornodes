{
  "name": "server-raptornodes",
  "script": "npm",
  "args" : "start",
//  "cwd": "/usr/web/server-raptornodes/",
  "env": {
    "NODE_ENV": "production"
  },
//  "instance_var": "INSTANCE_ID",
